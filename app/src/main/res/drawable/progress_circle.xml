<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/progressBackground" android:left="5dp" android:top="5dp"
        android:width="70dp" android:height="70dp">
        <shape
            android:shape="oval">
        </shape>
    </item>
    <item android:width="80dp" android:height="80dp">
        <shape
            android:shape="ring"
            android:thicknessRatio="20"
            android:useLevel="false">
            <solid android:color="#708BC34A"/>
        </shape>
    </item>
    <item android:width="80dp" android:height="80dp">
        <rotate
            android:fromDegrees="270"
            android:toDegrees="270">
            <shape
                android:shape="ring"
                android:thicknessRatio="20"
                android:useLevel="true">
                <solid android:color="#00F80A"/>
            </shape>
        </rotate>
    </item>
</layer-list>